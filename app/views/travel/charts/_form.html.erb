<%= simple_form_for(@travel_chart) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company_id %>
    <%= f.input :policy_for %>
    <%= f.input :trip_type %>
    <%= f.input :max_trip_duration %>
    <%= f.input :coverage %>
    <%= f.input :age_start %>
    <%= f.input :age_end %>
    <%= f.input :plan %>
    <%= f.input :duration_start %>
    <%= f.input :duration_end %>
    <%= f.input :has_usa %>
    <%= f.input :premium %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
