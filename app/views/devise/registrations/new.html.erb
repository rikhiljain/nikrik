<div id="deviseDiv" class="is_main_content_div">
  <div class="alert alert-block is_alert-heading is_main_content_header">
    <strong>Sign up</strong>
  </div>
  <div class="is_main_content_body">
    <%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs form-horizontal">
      <%= f.input :name, :required => true, :autofocus => true %>
      <%= f.input :email, :required => true %>
      <%= f.input :mobile, :required => true %>
      <%= f.input :address, :as => :text , :input_html => { :cols => 20, :rows => 5 } %>
      <%= f.input :password, :required => true %>
      <%= f.input :password_confirmation, :required => true %>
      <div class="control-group">
        <div class="controls">
          <%= f.button :submit, "Sign up", :class => "is_btn-custom" %>
          <%= render "devise/shared/links" %>
        </div>
      </div>    
    </div>
    <% end %>
  </div>
</div>
<% content_for :insuranceContent do %>
  <%= render :partial => "welcome/index", :locals => { :display => "true" } %>
<% end %>
<%= javascript_tag do %>
  window.content = "devise";
<% end %>   