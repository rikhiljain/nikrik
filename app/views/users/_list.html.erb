<h3>Users</h3>
<div class="span8">
  <table class="table table-condensed">
    <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Mobile</th>
      <th>Address</th>
      <th>Register Date</th>
    </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
        <tr>
          <td><%= link_to user.name, user %></td>
          <td><%= user.email %></td>
          <td><%= user.mobile %></td>
          <td><%= user.address %></td>
          <td><%= user.created_at.to_date %></td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>