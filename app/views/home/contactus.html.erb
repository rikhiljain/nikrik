<div id="deviseDiv" class="is_main_content_div">
  <div class="alert alert-block is_alert-heading is_main_content_header">
    <strong>Send An Advisor</strong>
  </div>
  <div class="is_main_content_body">
    <%= simple_form_for @contactus, :url => url_for(:action => 'contactus', :controller => 'home'), :html => {:method => :post } do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs form-horizontal" >
      <%= f.input :product, :required => true, :autofocus => true, :prompt => "Select Product Type",     :collection => [ "MOTOR", "HEALTH", "TRAVEL"] %>
       <%= f.input :name, :required => true %>
      <%= f.input :address, :required => true, :as => :text , :input_html => { :cols => 20, :rows => 5 } %>
       <%= f.input :email, :required => true %>
      <%= f.input :mobile, :required => true %>
      <%= f.input :message, label: 'Reason', :required => true, :as => :text , :input_html => { :cols => 20, :rows => 5 } %>
      <div class="controls">
        <%= f.button :submit, "Submit", :class => "is_btn-custom" %>
      </div>        
    </div>
    <% end %>
  </div>
</div>
<% content_for :insuranceContent do %>
  <%= render :partial => "welcome/index", :locals => { :display => "none" } %>
<% end %>
<%= javascript_tag do %>
  window.content = "devise";
<% end %> 
