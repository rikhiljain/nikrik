<div class="is_right-hand-div">
	<div class="alert alert-block is_alert-heading">
		<strong>Want us to call you</strong>
	</div>
	<div id="callUsFormNotificationDiv">
	</div>
	<div>
		<form method="post" action="" id="callUsForm" >
			  <fieldset>
				   <div class="control-group">
  						<label class="control-label" for="mobile_number">Mobile Number</label>
				  		<div class="controls">
				    		<input type="text" id="mobile_number" name="mobile_number" placeholder="mobile number"></input>
				    	</div>
				    </div>    
				    <div class="control-group">
  						<label class="control-label" for="email_address">Email address</label>
				  		<div class="controls">
				    		<input type="text" id="email_address" name="email_address" placeholder="abc@gmail.com"></input>
				    	</div>
				    </div>  
				    <div class="control-group">
						<div class="controls">
							<button type="submit" class="btn is_btn-custom">Submit</button>
						</div>
					</div>  
			  </fieldset>
		</form>
	</div>
</div>