<%= simple_form_for(@motor_discount) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :idv_chart_id %>
    <%= f.input :company_id %>
    <%= f.input :amount %>
    <%= f.input :rto_code %>
    <%= f.input :rto_sub_code %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
