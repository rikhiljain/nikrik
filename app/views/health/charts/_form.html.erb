<%= simple_form_for(@health_chart) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company_id %>
    <%= f.input :coverage %>
    <%= f.input :age_start %>
    <%= f.input :age_end %>
    <%= f.input :premium %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
